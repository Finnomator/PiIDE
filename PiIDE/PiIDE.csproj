  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Jedi\code_completer.exe" />
    <None Remove="Assets\Lint\pylint.exe" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Assets\Jedi\code_completer.exe">

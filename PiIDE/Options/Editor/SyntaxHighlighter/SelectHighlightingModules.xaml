<UserControl x:Class="PiIDE.Options.Editor.SyntaxHighlighter.SelectHighlightingModules"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PiIDE.Options.Editor.SyntaxHighlighter"
             mc:Ignorable="d" 
             Unloaded="UserControl_Unloaded"
             d:DesignHeight="450" d:DesignWidth="800">
    <Border BorderThickness="1" BorderBrush="{StaticResource SplitterBackgroundBrush}">
        <StackPanel>
            <CheckBox Name="JediNamesCheckBox" Click="JediNamesCheckBox_Click"  Content="Jedi Names"/>
            <CheckBox Name="KeywordsCheckBox" Click="KeywordsCheckBox_Click"  Content="Keywords"/>
            <CheckBox Name="BracketsCheckBox" Click="BracketsCheckBox_Click" Content="Brackets"/>
        </StackPanel>
    </Border>
</UserControl>
